{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "endpoint": "http://localhost:80",
  "allowedOrigins": "http://localhost:4200,http://localhost:4201",
  "AzureAdB2C": {
    "Instance": "https://datasynchrob2c.b2clogin.com",
    "ClientId": "[ClientId]", //App Id for Backend API App Reg
    "Domain": "datasynchrob2c.onmicrosoft.com",
    "SignUpSignInPolicyId": "B2C_1_SignUpIn",
    "TenantId": "[TenantId]"
  },
  "AzureAd": {
    "Instance": "https://login.microsoftonline.com",
    "ClientId": "[ClientId]", //App Id for Backend API App Reg, can be B2C app reg or AAD app reg
    "Domain": "datasynchrob2c.onmicrosoft.com", //Tenant where App reg was created, doesnt have to be the same as front end
    "TenantId": "[TenantId]" //TenantId where App reg was created, doesnt have to be the same as front end
  }
}